amends "../AppConfig.pkl"

grpc {
  serviceName = "go-skeleton-app-grpc-api"
  host = "127.0.0.1"
  port = 8081
  keepAlive {
    enabled = true
    policy {
      minTime = 5
      permitWithoutStream = true
    }
    parameter {
      maxConnectionIdle = 15
      maxConnectionAge = 30
      maxConnectionAgeGrace = 5
      time = 5
      timeout = 1
    }
  }
}
http {
