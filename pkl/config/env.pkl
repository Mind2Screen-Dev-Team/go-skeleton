amends "../AppConfig.pkl"

appName = "Go-Skeleton"
appDomain = "localhost"
appHost = "127.0.0.1"
appJwtSecret = ""
appLogPath = "../storage/logs"

appHttp {
  port = 1234
  idleTimeout = 30
  readHeaderTimeout = 5
  readTimeout = 15
  writeTimeout = 20
  handlerTimeout = 60
}

redis {
  enabled = true
  host = "127.0.0.1"
  port = 6379
  auth {
    username = "user"
    password = "example"
  }
}

nats {
  enabled = true
  host = "127.0.0.1"
  port = 4444
  auth {
    username = "user"
    password = "example"
  }
}

mysql {
  enabled = true
  host = "127.0.0.1"
  port = 3306
  db = "mysql-db"
  auth {
    username = "user"
    password = "example"
  }
}